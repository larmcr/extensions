{
  "manifest_version": 2,
  "name": "GN Popup Removal",
  "description": "Remove popups and allow to watch the embedded videos of GN sites",
  "version": "1.0",
  "author": "Luis Rojas",

  "icons": {
    "128": "icon.png"
  },

  "content_scripts": [
    {
        "matches": [
          "http://www.aldia.cr/*",
          "http://www.elfinancierocr.com/*",
          "http://www.lateja.co.cr/*",
          "http://www.nacion.com/*",
          "http://www.revistaperfil.com/*",
          "http://www.yuplon.com/*"
        ],

        "js": ["minified-web.js", "main.js"]
    }
  ]
}
